        "billing",
        "hiapi",
        "tariff plan"
    ],
    "homepage": "https://github.com/hiqdev/billing-hiapi",
    "license": "BSD-3-Clause",
    "support": {
        "email": "support@hiqdev.com",
        "source": "https://github.com/hiqdev/billing-hiapi",
        "issues": "https://github.com/hiqdev/billing-hiapi/issues",
        "wiki": "https://github.com/hiqdev/billing-hiapi/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
