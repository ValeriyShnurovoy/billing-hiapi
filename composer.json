    "require": {
        "hiqdev/hiapi": "dev-master",
        "lcobucci/content-negotiation-middleware": "^3.0",
        "hiqdev/php-billing": "dev-master",
        "woohoolabs/yin": "^4.1",
        "hoa/ruler": "^2.17"
    },
    "require-dev": {
        "yiisoft/yii2": "dev-master",
        "laminas/laminas-diactoros": "^2.1",
        "hiqdev/hidev": "dev-master",
        "hiqdev/hidev-php": "dev-master",
        "hiqdev/hidev-hiqdev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\billing\\hiapi\\": "src",
            "hiqdev\\billing\\hiapi\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "1.0": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "console": "config/console.php",
            "web": "config/web.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
